//  399 
//  400 static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault );
//  401 
//  402 
//  403 /*********************************************************************
//  404  * @fn       zgItemInit()
//  405  *
//  406  * @brief
//  407  *
//  408  *   Initialize a global item. If the item doesn't exist in NV memory,
//  409  *   write the system default (value passed in) into NV memory. But if
//  410  *   it exists, set the item to the value stored in NV memory.
//  411  *
//  412  *   Also, if setDefault is TRUE and the item exists, we will write
//  413  *   the default value to NV space.
//  414  *
//  415  * @param   id - item id
//  416  * @param   len - item len
//  417  * @param   buf - pointer to the item
//  418  * @param   setDefault - TRUE to set default, not read
//  419  *
//  420  * @return  ZSUCCESS if successful, NV_ITEM_UNINIT if item did not
//  421  *          exist in NV, NV_OPER_FAILED if failure.
//  422  */
//  423 static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault )
//  424 {
//  425 
//  426   uint8 status;
//  427 
//  428   // If the item doesn't exist in NV memory, create and initialize
//  429   // it with the value passed in.
//  430   status = osal_nv_item_init( id, len, buf );
//  431   if ( status == ZSUCCESS )
//  432   {
//  433     if ( setDefault )
//  434     {
//  435       // Write the default value back to NV
//  436       status =  osal_nv_write( id, 0, len, buf );
//  437     }
//  438     else
//  439     {
//  440       // The item exists in NV memory, read it from NV memory
//  441       status = osal_nv_read( id, 0, len, buf );
//  442     }
//  443   }
//  444 
//  445   return (status);
//  446 }
